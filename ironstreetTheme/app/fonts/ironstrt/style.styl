@import "variables"

@font-face
    font-family 'ironstrt'
    src url('fonts/ironstrt.eot?v1y9m9')
    src url('fonts/ironstrt.eot?v1y9m9#iefix') format('embedded-opentype'), url('fonts/ironstrt.ttf?v1y9m9') format('truetype'), url('fonts/ironstrt.woff?v1y9m9') format('woff'), url('fonts/ironstrt.svg?v1y9m9#ironstrt') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
    font-family 'ironstrt' !important
    speak none
    font-style normal
    font-weight normal
    font-variant normal
    text-transform none
    line-height 1

    /* Better Font Rendering =========== */
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

